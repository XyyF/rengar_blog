<!--主题入口-->
<!--config变量，根目录下的_config.yml配置项-->

<!--html-head标签头部信息-->
<%- partial('_partial/html-head/index') %>

<body>
<div id="container"
     v-loading="!renderContainer"
     class="container-none"
     :class="{ 'tools-show': isShow, 'container-show': renderContainer }">
    <!--tools展开的背景动画-->
    <canvas id="anm-canvas" class="anm-canvas"></canvas>

    <!--最左侧个人信息展示栏-->
    <div class="left-col" :class="{ 'tools-show': isShow }">
        <%- partial('_partial/left-col/index', null, {cache: !config.relative_link}) %>
    </div>

    <div class="mid-col" :class="{ 'tools-show':isShow, 'hide': isFalse(isShow) }">
        <!--手机端-->
        <%- partial('_partial/mobile-nav', null, {cache: !config.relative_link}) %>
        <div id="wrapper" class="body-wrap">
            <div class="menu-l">
                <div class="canvas-wrap">
                    <canvas data-colors="#eaeaea" data-sectionHeight="100" data-contentId="js-content" id="myCanvas1" class="anm-canvas"></canvas>
                </div>
                <div id="js-content" class="content-ll">
                    <!--./index.ejs或者page.ejs的内容-->
                    <%- body %>
                </div>
            </div>
        </div>

        <!--底部联系我栏-->
        <%- partial('_partial/footer/index') %>
    </div>

    <!--标签栏-->
    <%- partial('_partial/tools/index') %>

    <!--图片预览-->
    <%- partial('_partial/viewer') %>

    <!--dialog弹窗栏-->
    <rg-dialog ref="dialog"></rg-dialog>
</div>

<!--设置全局配置window.yiliaConfig-->
<%- partial('_partial/after-footer') %>

</body>
</html>
